package Operator;

public class OperatorEx1 {

	public static void main(String[] args) {
		//변수값 교환, 대입연산자('=')
		int x =0;
		int y =1;
		
		System.out.println("*****교환전******");
		System.out.println("x="+x+",y="+y);
		
		//연산(교환)
		int temp;//교환용 변수
		temp =x;
		x=y;
		y=temp;
		
		

		System.out.println("*****교환후******");
		System.out.println("x="+x+",y="+y);
	}

}

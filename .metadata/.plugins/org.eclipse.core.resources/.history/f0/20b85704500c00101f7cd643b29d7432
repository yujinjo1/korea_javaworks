package choice;

/*
 * @학점 계산 프로그램
 * 90~100:A
 * 80~89:B
 * 70~79:C
 * 70점 미만: F
 * 
 * */
public class IfElseIfElse {

	public static void main(String[] args) {
		// 다중 조건문
		//논리 연산자 - &&, ||, !
		int score = 86;
		char grade=''; //하나의 문자를 저장할 변수 grade 선언
		
		if(score>=90&&score<=100) {
			grade='A';
			
		}
		System.out.println("학점은"+grade+"입니다.");
	}

}
